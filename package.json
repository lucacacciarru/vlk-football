{
  "name": "vlk-football",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^1.8.8",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@fontsource/nunito": "^4.5.8",
    "@fontsource/poppins": "^4.5.8",
    "@reduxjs/toolkit": "^1.8.1",
    "@types/jest": "^27.5.0",
    "@types/node": "^16.11.33",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "framer-motion": "^6.3.3",
    "i18next": "^21.8.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^13.2.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^11.16.9",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "redux-saga": "^1.1.3",
    "typescript": "^4.6.4",
    "web-vitals": "^2.1.4",
    "wretch": "^1.7.9"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "json-server": "json-server --port 7000 --watch data/db.json",
    "eject": "react-scripts eject",
    "test:ci": "npm run test:coverage -- --testResultsProcessor=\"jest-junit\"",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "react-scripts test --watchAll=true"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.1.1"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "jest": {
    "coverageReporters": [
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 80,
        "branches": 80,
        "functions": 80,
        "statements": 80
      }
    },
    "collectCoverageFrom": [
      "<rootDir>/src/**/**/*.ts",
      "<rootDir>/src/**/**/*.tsx",
      "!<rootDir>/src/**/**/*.d.ts"
    ]
  }
}
